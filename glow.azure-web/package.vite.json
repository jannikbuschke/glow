    "@storybook/addon-links": "6.3.2",
    "@storybook/cli": "6.3.2",
    "@storybook/node-logger": "6.3.2",
    "@storybook/react": "6.3.2",
    "@types/node": "14.14.10",
    "@types/react-dom": "17.0.4",
    "@types/react-router-dom": "5.1.6",
    "@types/react-router": "5.1.8",
    "@types/react": "17.0.5",
    "@vitejs/plugin-react-refresh": "1.3.3",
    "npm-run-all": "4.1.5",
    "prettier": "2.3.0",
